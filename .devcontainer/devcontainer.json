{
  "name": "adventofcode",
  "image": "mcr.microsoft.com/devcontainers/javascript-node",
  "hostRequirements": {
    "cpus": 2
  },
  "onCreateCommand": {
    "link": "npm link",
    "dependencies": "npm install"
  },
  "remoteEnv": {
    "NO_FILE_OPEN": "true",
    "SESSION_COOKIE": "${localEnv:AOC_SESSION_ID}"
  }
}
